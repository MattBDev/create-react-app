{
  "compilerOptions": {
    "moduleResolution": "Bundler",
    "module": "ES2020",
    "target": "ES2022",
    "allowImportingTsExtensions": true,
    "esModuleInterop": true,
    "noEmit": true,
    "lib": [
      "DOM",
      "ESNext"
    ],
    "jsx": "react-jsx",
  },
  "include": [
    "src/**/*"
    // Add the file paths here
  ],
  "exclude": ["node_modules"]
}
